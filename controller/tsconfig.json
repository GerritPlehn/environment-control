{
	"compilerOptions": {
		"noEmit": true,
		"target": "esnext",
		"module": "nodenext",
		"rewriteRelativeImportExtensions": true,
		"erasableSyntaxOnly": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
		"lib": ["es2022"]
	}
}
